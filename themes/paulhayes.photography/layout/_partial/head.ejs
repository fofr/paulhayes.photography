<meta charset="UTF-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
<%
var title = page.title;

if (is_archive()){
  title = 'All photos';
  if (is_month()){
    title = 'Archives: ' + page.year + '/' + page.month;
  } else if (is_year()){
    title = 'Archives: ' + page.year;
  }
} else if (is_category()){
  title = page.category.charAt(0).toUpperCase() + page.category.slice(1)
} else if (is_tag()){
  titleTag = page.tag.split('-').join(' ');
  title = titleTag.charAt(0).toUpperCase() + titleTag.slice(1) + ' photos';
}
%>
<title><% if (title){ %><%= title %> — <% } %><%= config.title %></title>
<meta name="description" content="Paul Hayes – Brighton and Sussex Nature Photographer" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<script>(function(d){d.className=d.className.replace(/^no-js\b/,'js');}(document.documentElement));</script>
<link rel="stylesheet" href="/css/master.css?<%= config.site_build %>" />
<link rel="alternate" href="<%- theme.rss %>" title="<%= config.title %>" type="application/atom+xml">
<link rel="icon" type="image/png" href="/favicon.png" />
<script src="//use.typekit.net/tfj8peg.js"></script>
<script>try{Typekit.load();}catch(e){}</script>
<% if (is_post()) { %>
  <%- partial('_partial/photo/og', {photo: page}) %>
  <%- partial('_partial/photo/twitter-card', {photo: page}) %>
<% } else { %>
  <%- partial('_partial/meta-tags') %>
<% } %>
